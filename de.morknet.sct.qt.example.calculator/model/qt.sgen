GeneratorModel for de::morknet::sct::qt::generator
{
	statechart Calculator
	{
		feature LicenseHeader
		{
			licenseText =  "Copyright (C) 2017 - Steffen A. Mork" 
		}

		feature Outlet
		{
			targetProject = "de.morknet.sct.qt.example.calculator"
			targetFolder = "src-gen"
			libraryTargetFolder = "src"
		}

		feature QtSettings
		{
			baseClassName = "AbstractCalculator"
			className = "AbstractCalculatorDispatcher"
			cpp11 = true
			generateTimer = true
			threadSafe = false
			debug = true
			camelCase = true
		}
	}
}
