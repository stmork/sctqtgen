<?xml version="1.0" encoding="ASCII"?>
<sgen:FeatureTypeLibrary xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:sgen="http://www.yakindu.org/sct/statechart/SGen" name="Qt-Generator for SCT">
  <types name="QtSettings">
    <parameters name="baseClassName" comment="This parameter defines the base class name of the generated code."/>
    <parameters name="className" comment="This parameter defines the generated class name."/>
    <parameters name="cpp11" comment="This parameter controls whether the generator should produce C++11 compatible source code." optional="true" parameterType="BOOLEAN"/>
    <parameters name="generateTimer" comment="This parameter lets the generator produce a QtTimer class used for the statemachines timer implementation." optional="true" parameterType="BOOLEAN"/>
    <parameters name="threadSafe" comment="This parameter controls whether the generated source should be thread safe. Note that this option works only together with C++11." optional="true" parameterType="BOOLEAN"/>
    <parameters name="debug" comment="This parameter controls debug logging and generating some information files." optional="true" parameterType="BOOLEAN"/>
    <parameters name="camelCase" comment="This parameter decides the naming of Qt signals." optional="true" parameterType="BOOLEAN"/>
    <parameters name="qtBaseClass" comment="The Qt base class to use. This is QObject by default." optional="true"/>
  </types>
</sgen:FeatureTypeLibrary>
