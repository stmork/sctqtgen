GeneratorModel for de::morknet::sct::qt::generator
{
	statechart Timerless
	{
		feature LicenseHeader
		{
			licenseText =  "Copyright (C) 2020 - Steffen A. Mork" 
		}

		feature QtSettings
		{
			baseClassName = "AbstractStateMachine"
			className = "AbstractStateMachineDispatcher"
			cpp11 = true
			generateTimer = true
			threadSafe = false
			debug = true
		}

		feature Outlet
		{
			targetProject = "de.morknet.sct.qt.example.timerless"
			targetFolder = "src-gen"
			libraryTargetFolder = "src"
		}
	}
}
